{
    "manifest_version": 3,
    "name": "Twitch profile pic upscale",
    "version": "0.0.2",
    "description": "See Twitch profile pics in big POGGERS",
    "action": {
        "default_popup": "index.html",
        "default_title": "Twitch profile pic upscale"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": [
        "https://twitch.tv/*",
        "https://static-cdn.jtvnw.net/*"
    ]
}